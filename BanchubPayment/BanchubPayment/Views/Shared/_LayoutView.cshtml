<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <!-- Apple devices fullscreen -->
    <meta name="apple-mobile-web-app-capable" content="yes" />
   @* <meta http-equiv="refresh" content="0; url=../Error/ErrorPage" />*@
    <!-- Apple devices fullscreen -->
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
     <!-- Bootstrap -->
    
    <link href="~/CSS/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="../CSS/font-awesome.min.css" rel="stylesheet" />
    <link href="../Content/theme/style.css" rel="stylesheet" />
    <!-- jQuery UI -->
    <link href="../Content/theme/jquery-ui.min.css" rel="stylesheet" />
    <!-- Color CSS -->
    <link href="../Content/theme/theme.css" rel="stylesheet" />

    @*<link href="~/notify/css/animate.css" rel="stylesheet" />
    <link href="~/notify/css/app.css" rel="stylesheet" />
    <link href="~/notify/css/pnotify.custom.css" rel="stylesheet" />
    <link href="~/notify/css/pnotify.custom.min.css" rel="stylesheet" />*@

    <title>@ViewBag.Title</title>
    @*<link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>*@
</head>

<body style="min-width:350px; overflow:auto;background-color:#ECF0F1;font-family:Arial, Verdana !important">
    <header></header>
    <div class="container" style="margin-top:10%;">

        <div class="row">
            <div class="col-md-1 col-sm-12 cols-xs-6"></div>
            <div class="col-md-10 col-sm-12 cols-xs-6">
                @RenderSection("featured", required: false)
                @RenderBody()

            </div>
            <div class="col-md-1 col-sm-12 cols-xs-6">
            </div>
        </div>

    </div>


    <!-- jQuery -->
    <script src="~/js/jquery.min.js"></script>
    <!-- Nice Scroll -->
    <script src="~/JS/jquery.nicescroll.min.js"></script>
    <!-- jQuery UI -->
    <script src="~/JS/theme/jquery-ui.js"></script>
    <!-- Bootstrap -->
    @*	<script src="js/bootstrap.min.js"></script>*@
    <script src="~/JS/bootstrap/bootstrap.min.js"></script>
    <!-- Chosen -->
    <script src="~/JS/theme/chosen.jquery.min.js"></script>
    <!-- select2 -->
    <script src="~/JS/theme/select2.min.js"></script>
    <!-- icheck -->
    <script src="~/JS/theme/jquery.icheck.min.js"></script>
    <!-- Theme framework -->
    <script src="~/JS/theme/eakroko.min.js"></script>
    <!-- Theme scripts -->
    <script src="~/JS/theme/application.min.js"></script>
    <!-- Theme scripts -->
    <script src="~/JS/validation.js"></script>
    <script src="~/JS/aes.js"></script>

    <!--Animate themes-->
    @*<script src="~/notify/js/app.js"></script>
        <script src="~/notify/js/pnotify.custom.js"></script>
        <script src="~/notify/js/pnotify.custom.min.js"></script>*@

</body>
</html>
